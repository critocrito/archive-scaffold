{
  "name": "Twitter Feeds",
  "extends": [
    "./configs/downloads.json",
    "./configs/mongodb.json",
    "./configs/elasticsearch.json",
    "./configs/mail.json",
    "./configs/csv.json",
    "./configs/secrets.json"
  ],
  "plugins": [
    "sheets_queries",
    "workflow_multiplex",
    "twitter_feed",
    "media_youtubedl",
    "http_get",
    "mongodb_supplement",
    "cid_annotate",
    "mongodb_store",
    "elastic_supplement",
    "enigio_timestamp",
    "scrub_omit_fields",
    "elastic_export",
    "workflow_multiplex_end",
    "csv_export_failed",
    "mail_pipeline_stats",
    "mail_failed_stats",
    "tap_printf"
  ],
  "google": {
    "query_default_type": "twitter_user",
    "query_fields": [
      "record owner",
      "location",
      "activity",
      "cid.incident_code",
      "notes"
    ],
    "selection_list": ["type:twitter_user"]
  },
  "media": {
    "youtubedl_parallel": 6
  },
  "workflow": {
    "multiplex_continue_on_error": true,
    "multiplex_size": 100,
    "merge_query_fields": ["cid.incident_code", "notes"]
  },
  "tap": {
    "select": "stats"
  }
}
