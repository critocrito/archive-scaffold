    "./configs/downloads.json",
    "./configs/mongodb.json",
    "./configs/elasticsearch.json",
    "./configs/mail.json",
    "./configs/csv.json",
    "./configs/query-handling.json",
    "./configs/secrets.json"
  ],
  "plugins": [
    "liveuamap_region",
    "twitter_tweets",
    "youtube_video",
    "media_youtubedl",
    "mongodb_supplement",
    "media_mosaic",
    "http_get",
