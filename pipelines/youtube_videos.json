{
  "name": "Youtube Videos",
  "extends": [
    "./configs/downloads.json",
    "./configs/mongodb.json",
    "./configs/elasticsearch.json",
    "./configs/mail.json",
    "./configs/secrets.json"
  ],
  "plugins": [
    "sheets_queries",
    "youtube_video",
    "media_youtubedl",
    "mongodb_supplement",
    "mongodb_store",
    "elastic_supplement",
    "scrub_omit_fields",
    "elastic_export",
    "sheets_move_queries",
    "mail_pipeline_stats",
    "mail_failed_stats",
    "tap_printf"
  ],
  "google": {
    "query_default_type": "youtube_video",
    "query_types_to_move": "youtube_video",
    "query_fields": ["record owner", "description"],
    "selection_list": ["type:youtube_video"]
  },
  "media": {
    "youtubedl_parallel": 6
  },
  "tap": {
    "select": "stats"
  },
  "workflow": {
    "multiplex_size": 1000
  }
}
