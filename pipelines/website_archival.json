{
  "name": "Facebook Posts",
  "extends": [
    "./configs/mongodb.json",
    "./configs/elasticsearch.json",
    "./configs/mail.json",
    "./configs/csv.json",
    "./configs/query-handling.json",
    "./configs/instruments.json",
    "./configs/secrets.json"
  ],
  "plugins": [
    "sheets_queries",
    "workflow_multiplex",
    "http_import",
    "mongodb_supplement",
    "cid_annotate",
    "workflow_merge_query",
    "mongodb_store",
    "elastic_supplement",
    "enigio_timestamp",
    "scrub_pick_fields",
    "elastic_export",
    "sheets_move_queries",
    "workflow_multiplex_end"
  ],
  "google": {
    "query_default_type": "http_url",
    "query_types_to_move": "http_url",
    "selection_list": ["type:http_url"]
  },
  "workflow": {
    "multiplex_size": 20
  }
}
