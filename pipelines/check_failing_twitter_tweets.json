{
  "name": "Twitter Failing Tweets",
  "extends": [
    "./configs/media.json",
    "./configs/mongodb.json",
    "./configs/csv.json",
    "./configs/mail.json",
    "./configs/instruments.json",
    "./configs/secrets.json"
  ],
  "plugins": [
    "mongodb_query_units",
    "twitter_filter_failing",
    "mail_failed_stats"
  ],
  "queries": [
    {
      "type": "mongodb_query_units",
      "term": { "_sc_source": { "$in": ["twitter_feed", "twitter_tweets"] } }
    }
  ]
}
