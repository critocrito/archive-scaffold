{
  "name": "Facebook Posts",
  "extends": [
    "./configs/media.json",
    "./configs/downloads.json",
    "./configs/mongodb.json",
    "./configs/elasticsearch.json",
    "./configs/mail.json",
    "./configs/csv.json",
    "./configs/query-handling.json",
    "./configs/instruments.json",
    "./configs/secrets.json"
  ],
  "plugins": [
    "sheets_queries",
    "workflow_multiplex",
    "facebook_post",
    "media_youtubedl",
    "http_get",
    "mongodb_supplement",
    "media_mosaic",
    "cid_annotate",
    "workflow_merge_query",
    "mongodb_store",
    "elastic_supplement",
    "enigio_timestamp",
    "scrub_pick_fields",
    "elastic_export",
    "sheets_move_queries",
    "workflow_multiplex_end",
